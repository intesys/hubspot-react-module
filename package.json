{
  "name": "hubspot-react-module",
  "version": "1.0.0",
  "targets": {
    "hubspot": {
      "context": "browser",
      "includeNodeModules": true,
      "distDir": ".",
      "optimize": true,
      "isLibrary": false
    }
  },
  "scripts": {
    "dev": "parcel watch src/module.tsx --target hubspot --no-content-hash --no-hmr",
    "build": "parcel build src/module.tsx --target hubspot --no-content-hash"
  },
  "bin": {
    "create-hubspot-react-module": "./bin/install.js",
    "new": "./bin/install.js"
  },
  "author": "intesys",
  "license": "GPL-3.0-only",
  "description": "An utility to create a react module for HubSpot",
  "dependencies": {
    "parcel": "^2.14.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.23.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "eslint": "^9.23.0",
    "eslint-plugin-react": "^7.37.4",
    "fs-extra": "^11.2.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0"
  }
}